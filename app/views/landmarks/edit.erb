<h1>Edit <%= @landmark.name %></h1>
<form action="/landmarks/<%= @landmark.id %>" method="post">
<p><label for="landmark[name]">Name: </label><input type="text" name="landmark[name]" id="name" value="<%= @landmark.name %>"></p>
<p><label for="landmark[year_completed]">Year: </label><input type="text" name="landmark[year_completed]" id="year_completed" value="<%= @landmark.year_completed %>"></p>
<input type="submit" value="Edit Landmark">
</form>